//verify if a number is prime;

int checkedNumber;

checkedNumber = read();

int isPrime = 1;

int currentNumber = -2;

while(currentNumber < checkedNumber){
	if( checkedNumber % currentNumber == 0){
		isPrime = 0;
	};
	currentNumber = currentNumber+1;

};

if(isPrime == 1){
	print("prime");
	
} else {
	print("not prime");
};